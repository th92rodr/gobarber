{
  "name": "gobarber",
  "version": "1.0.0",
  "author": "Thiago Rodrigues",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev:server": "ts-node-dev --transpileOnly --ignore node_modules/ src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "typeorm:migration:create": "ts-node-dev ./node_modules/typeorm/cli.js migration:create --name",
    "typeorm:migration:run": "ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "typeorm:migration:revert": "ts-node-dev ./node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.14.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "uuidv4": "^6.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.3",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5"
  }
}
